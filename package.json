{
  "name": "headlines",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon --verbose -e css,ejs,js bin/www",
    "run": "mongod --fork --logpath ./log/mongodb.log --dbpath ./data && nodemon --verbose -e css,ejs,js bin/www && mongod --shutdown --dbpath ./data",
    "structure": "tree -I 'node_modules|log|data'",
    "clear": "rm -r log data && mkdir log data",
    "test": "node_modules/.bin/mocha --reporter spec --require test/bootstrap/node test/*.test.js"
  },
  "dependencies": {
    "bcryptjs": "^0.7.12",
    "body-parser": "~1.15.1",
    "connect-flash": "~0.1.1",
    "cookie-parser": "~1.4.3",
    "cron": "^1.2.1",
    "debug": "~2.2.0",
    "ejs": "~2.4.1",
    "express": "~4.13.4",
    "express-session": "latest",
    "feedparser": "^2.1.0",
    "mongodb": "^2.2.25",
    "morgan": "~1.7.0",
    "nodemon": "^1.11.0",
    "passport": "~0.2.1",
    "passport-local": "~1.0.0",
    "request": "^2.81.0",
    "serve-favicon": "^2.3.2"
  }
}
